<cu-custom bgColor="bg-gradual-pink" isBack='{{false}}'>
  <view slot="content">首页</view>
</cu-custom>



<view class="container">
  <view class='nav-list {{jsmember?"margin-top-xl":"mymargin"}}'>
    <navigator open-type="navigate" url="/pages/{{item.name}}/{{item.name}}" class="nav-li bg-{{item.color}}"
      wx:for="{{elements}}" wx:key>
      <view class="nav-title margin">{{item.title}}</view>
      <view class="nav-name margin">{{item.name}}</view>
      <text class='cuIcon-{{item.icon}} margin'></text>
    </navigator>

    <navigator open-type="navigate" url="/pages/{{item.name}}/{{item.name}}" class="nav-li bg-{{item.color}}"
      wx:for="{{elements2}}" wx:key wx:if="{{jsmember}}">
      <view class="nav-title margin">{{item.title}}</view>
      <view class="nav-name margin">{{item.name}}</view>
      <text class='cuIcon-{{item.icon}} margin'></text>
    </navigator>

  </view>
</view>