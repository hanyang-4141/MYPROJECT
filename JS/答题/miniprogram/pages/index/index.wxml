<cu-custom bgColor="bg-gradual-pink" isBack='{{false}}'>
  <view slot="content">首页</view>
</cu-custom>



<view class="container">
  <view class='nav-list margin-top-xl'>
    <!-- <navigator open-type="navigate" url="/pages/{{item.name}}/{{item.name}}" class="nav-li bg-{{item.color}}"
      wx:for="{{elements}}" wx:key='id'>
      <view class="nav-title margin">{{item.title}}</view>
      <view class="nav-name margin">{{item.name}}</view>
      <text class='cuIcon-{{item.icon}} margin'></text>
    </navigator> -->

    <block wx:for="{{elements}}" wx:key='id'>
      <block wx:if="{{index<4}}">
        <navigator hidden="{{!jsmember}}" open-type="navigate" url="{{jsmember?'/pages/' + item.name + '/' + item.name + '?id=' + item.name:''}}" class="nav-li bg-{{item.color}}" >
          <view class="nav-title margin">{{item.title}}</view>
          <view class="nav-name margin">{{item.name}}</view>
          <text class='cuIcon-{{item.icon}} margin'></text>
        </navigator>        
      </block>
      <block wx:else>
        <navigator open-type="navigate" url="/pages/{{item.name}}/{{item.name}}?questionTitle={{item.title}}" class="nav-li bg-{{item.color}}" >
          <view class="nav-title margin">{{item.title}}</view>
          <view class="nav-name margin">{{item.name}}</view>
          <text class='cuIcon-{{item.icon}} margin'></text>
        </navigator>  
      </block>
      

    </block>

    <!-- <navigator open-type="navigate" url="/pages/{{item.name}}/{{item.name}}" class="nav-li bg-{{item.color}}"
