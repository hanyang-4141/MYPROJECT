.txt {
  font-weight: bold;
}

.arrow{
  width: 12px;
  height: 12px;
  border-top: 2px solid rgba(128, 128, 128, 0.726);
  border-right: 2px solid rgba(128, 128, 128, 0.726);
  transform: rotate(45deg);
}
.arrow-view{
  width: 50%;
}
.feedback{  
  margin: 0;
  padding: 0;
  background-color: transparent;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: start;
  align-items: center;
  
  font-weight: normal;
  flex-grow: 1;
}
.hehe{
  display: flex;
}